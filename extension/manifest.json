{
    "manifest_version": 3,
    "name": "GatorGatchi",
    "version": "1.0",
    "description": "A gator that helps you learn",
    "icons": {
      "128": "icon.png"
    },
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"] ,
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js" : ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "web_accessible_resources": [
    {
      "resources": ["gator.gif"],
      "matches": ["<all_urls>"]
    }
    ]
  }